						BanPage='http://www.vendors.com.ua/api/index/21-' + $UID$;

						$.get(ProfilePage,function(data) {

						function convertDate(elem, data) {
						var date = new Date( $(elem, data).text() * 1000 );

						return date.getDate() + '.' + date.getMonth() + '.' + date.getFullYear();
						}

						UserStatus = $('member:contains("USER_STATUS") string', data).text();
						UserName = $('member:contains("USER_NAME") string', data).text();
						UserAvatar = $('member:contains("USER_AVATAR") string', data).text();
						UserReputation = $('member:contains("USER_REPUTATION") string', data).text();
						UserBannedTill = $('member:contains("USER_BANNEDTILL") string', data).text();
						UserStatus = $('member:contains("USER_STATUS") string', data).text();

						if (UserStatus=='Online') {
						SellerStatus = 'seller-online';
						} else {
						SellerStatus = 'seller-offline';
						}

						$('.seller-name').append('<a href="$PROFILE_URL$" title="Смотреть профиль">' + UserName + '</a>'); 
						$('.seller-status').append('<span class="' + SellerStatus + '">' + UserStatus + "</span>");
						$('.seller-reputation b').append(UserReputation);

						if (UserAvatar=='') {
						$('.seller-avatar').append('<img src="http://s20.ucoz.net/a/16/2849.jpg" title="' + UserName + '" alt="' + UserName + '" />');
						} else {
						$('.seller-avatar').append('<img src="' + UserAvatar + '" title="' + UserName + '" alt="' + UserName + '" />');
						}

						function convertDate(elem, data) {
						var date = new Date( $(elem, data).text() * 1000 );

						return date.getDate() + '.' + date.getMonth() + '.' + date.getFullYear();
						}

						$('.seller-registration').append('Регистрация: ' + convertDate('member:contains("USER_REGTIMESTAMP") i4', data)); 
						$('.seller-last-visit').append('Был на сайте: ' + convertDate('member:contains("USER_LOGTIMESTAMP") i4', data));

						if (UserBannedTill != '0') {
						$('.seller-banned-till').append('Заблокирован до <br />' + UserBannedTill).parents('li').removeClass('seller-param__display-none'); 
						}
						},'xml');

						$.get(BanPage,function(data) {
						UserBanCount = 0;

						$('member:contains("BANCHANGE_VALUE")',data).each(function() {
